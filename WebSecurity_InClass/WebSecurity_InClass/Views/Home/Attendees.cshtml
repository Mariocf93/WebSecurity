@model IEnumerable<WebSecurity_InClass.Models.ViewModels.IPNVM>

@{
    ViewBag.Title = "Attendees";
}

<h2>Attendees</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.transID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sessionID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.amount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.paymentStat)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.transID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sessionID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.amount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.paymentStat)
        </td>
    </tr>
}

</table>
